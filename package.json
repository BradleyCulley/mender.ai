{
  "name": "mender.ai",
  "version": "0.0.0",
  "scripts": {
    "docusaurus": "docusaurus",
    "tina": "tinacms dev -c \"yarn start\" ",
    "start": "docusaurus start",
    "tina-build": "tinacms build && docusaurus build",
    "build": "NEXT_PUBLIC_HIDE_EDIT_BUTTON=1 docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "watch": "npm-watch",
    "spell": "spellchecker -d dictionary.txt -f 'docs/**/*.mdx' 'blog/**/*.mdx'",
    "spell:gen": "spellchecker --generate-dictionary -f 'docs/**/*.mdx' 'blog/**/*.mdx'"
    
  },
  "watch": {
    "tina": "config/**/*.json"
  },
  "dependencies": {
    "@docusaurus/core": "^2.4.0",
    "@docusaurus/plugin-google-gtag": "^2.4.0",
    "@docusaurus/preset-classic": "^2.4.0",
    "@docusaurus/theme-mermaid": "^2.4.0",
    "@mdx-js/react": "^1.6.22",
    "@tinacms/cli": "^1.2.2",
    "clsx": "^1.2.1",
    "npm-watch": "^0.11.0",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "spellchecker-cli": "^6.1.1",
    "tinacms": "^1.2.2",
    "title": "^3.5.3"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.4.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  }
}
